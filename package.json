		"react-router-dom": "^6.22.3",
		"sqlite3": "^5.1.7",
		"uuid": "^10.0.0"
	},
	"devDependencies": {
		"@emotion/react": "^11.11.4",
		"@emotion/styled": "^11.11.5",
		"@types/cors": "^2.8.17",
		"@types/express": "^4.17.21",
		"@types/jest": "^29.5.12",
		"@types/node": "^20.11.5",
		"@types/react": "^18.2.43",
		"@types/react-dom": "^18.2.17",
		"@types/uuid": "^10.0.0",
