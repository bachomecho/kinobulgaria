		"postcss": "^8.4.38",
		"tailwindcss": "^3.4.3",
		"ts-jest": "^29.1.2",
		"ts-loader": "^9.5.1",
		"ts-node": "^10.9.2",
		"tsx": "^4.7.0",
		"typescript": "^5.2.2",
