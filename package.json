{
	"name": "kinobulgaria",
	"private": true,
	"version": "0.0.0",
	"type": "module",
	"scripts": {
		"preview": "vite preview",
		"test": "npx jest",
		"client:dev": "vite",
		"server:dev": "npx tsx ./server/server.ts",
		"client:build": "vite build --base=/",
		"server:build": "tsc -p ./server/tsconfig.json",
